{% extends "::content.html.twig" %}

{% block contentHeaderModal %}
    Procesar Encomienda Por Salida
{% endblock %}
    
{% block javascripts %}
    {{ parent() }}
    <script>
        $(document).ready(function(){
           procesarEncomiendaPorSalida.funcionesAddOnload();	
        }); 
    </script>
{% endblock %}
           
{% block contentBodyModal %}

<form class="form-horizontal procesarEncomiendaPorSalida" id="procesarEncomiendaPorSalida" name="procesarEncomiendaPorSalida" action="{{ path(route) }}" >
    
    <input type="hidden" name="mensajeServidor" id="mensajeServidor" value="{{ mensajeServidor }}"></input>
    
    <div class="container-fluid">
   
    <div class="row-fluid">     
        <div class="span12">  
            {{ form_label(form.salida) }}
            {{ form_errors(form.salida) }}
            {{ form_widget(form.salida) }}
        </div>
    </div>
    <BR>       
    <div class="row-fluid"> 
        <div class="span12"> 
            <a id="manifiesto" href="{{ url('reporte-manifiesto_encomienda_full') }}" class="btn">Manif. Encomienda</a>
        </div>
    </div>
    <BR>   
    
    <div class="row-fluid">     
    <div class="span12"> 
        
    <div class="tabbable">
        <ul class="nav nav-tabs">
            <li class="active">
                <a href="#tab1" data-toggle="tab">Abordadas</a>
            </li>
            <li>
                <a href="#tab2" data-toggle="tab">Pendientes</a>
            </li>
        </ul>
        <div class="tab-content">
            <div class="tab-pane active" id="tab1">
                <table id="encomiendasAbordadasTable" class="table table-striped table-bordered table-hover">
