<form action="{{ path('embarcarEncomienda-case1') }}" method="post" {{ form_enctype(form) }}>
    
    <input type="hidden" name="mensajeServidor" id="mensajeServidor" value="{{ mensajeServidor }}"></input>
    {{ form_errors(form) }}
    
    <div class="container-fluid">
    <div class="row-fluid">
    <div class="span12">  
        <label> <strong> ¿Está seguro que desea embarcar la encomienda? </strong> </label>
    </div> 
    </div> 
    <div class="row-fluid">
    <div class="span12">  
    {{ form_label(form.salida) }}
    {{ form_errors(form.salida ) }}
    {{ form_widget(form.salida , {'attr': {'placeholder': 'Seleccione la salida.' } }) }}
    </div> 
    </div> 
    
    {{ form_rest(form) }}
    
    <script>	
        $(document).ready(function(){
            embarcarEncomienda.funcionesAddOnload();	
        }); 
    </script>
    
</form>