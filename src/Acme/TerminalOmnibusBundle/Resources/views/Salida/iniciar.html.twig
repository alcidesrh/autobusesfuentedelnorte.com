<form action="{{ path(route) }}" method="post" {{ form_enctype(form) }}>
    
    <input type="hidden" name="mensajeServidor" id="mensajeServidor" value="{{ mensajeServidor }}"></input>
    {{ form_errors(form) }}
    
    <div class="container-fluid">
    <div class="row-fluid">
    <div class="span12">  
        <label> <strong> ¿Está seguro que desea iniciar la salida? </strong> </label>
    </div> 
    </div> 
    <div class="row-fluid">
    <div class="span12">  
    {{ form_label(form.totalBoletosChequeados) }}
    {{ form_widget(form.totalBoletosChequeados) }}
    </div>
    </div>
    <div class="row-fluid">
    <div class="span12">  
    {{ form_label(form.totalBoletosNOChequeados) }}
    {{ form_widget(form.totalBoletosNOChequeados) }}
    </div>
    </div> 
    <div class="row-fluid">
    <div class="span12">  
    {{ form_label(form.totalBoletosPendientes) }}
    {{ form_widget(form.totalBoletosPendientes) }}
    </div>
    </div> 
    <div class="row-fluid">
    <div class="span12">  
    {{ form_label(form.totalEncomiendasEmbarcadas) }}
    {{ form_widget(form.totalEncomiendasEmbarcadas) }}
    </div> 
    </div>     
        
    </div> 
    {{ form_rest(form) }}

    
</form>